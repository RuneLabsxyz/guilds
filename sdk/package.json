{
  "name": "@runelabsxyz/guilds-sdk",
  "version": "0.1.0",
  "description": "Typed TypeScript SDK for RuneLabs Guilds contracts",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "scripts",
    "generated"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./errors": {
      "types": "./dist/errors/index.d.ts",
      "import": "./dist/errors/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./bindings": {
      "types": "./dist/bindings/index.d.ts",
      "import": "./dist/bindings/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "typecheck:examples": "tsc -p tsconfig.examples.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "sync:contracts": "node ./scripts/sync-contract-signature.mjs",
    "check:compat": "node ./scripts/check-contract-compat.mjs",
    "verify": "npm run typecheck && npm run typecheck:examples && npm run check:compat && npm run test && npm run build"
  },
  "keywords": [
    "starknet",
    "guilds",
    "runelabs",
    "sdk",
    "typescript"
  ],
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
